#include "rnc.h"

# suite normalize

static void _test(const char *expected, const char *denormal) {
    int actual_ret;
    char actual_val[32];

    actual_ret = rnc_normalize(actual_val, sizeof(actual_val), denormal);

    ck_assert_int_eq(0, actual_ret);
    ck_assert_str_eq(expected, actual_val);
}

# test iiiii
    _test("V", "IIIII");

# test iiii
    _test("IV", "IIII");

# test vv
    _test("X", "VV");
