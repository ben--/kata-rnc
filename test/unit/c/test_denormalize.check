#include "rnc.h"

# suite denormalize

# test iv
    int actual_ret;
    char actual_val[32] = { 'u', 'n', 'i', 't', 'i', 'a', 'l', 'i', 'z', 'e', 'd' };

    actual_ret = rnc_denormalize(actual_val, sizeof(actual_val), "IV");

    ck_assert_int_eq(0, actual_ret);
    ck_assert_str_eq("IIII", actual_val);

# test ix
    int actual_ret;
    char actual_val[32] = { 'u', 'n', 'i', 't', 'i', 'a', 'l', 'i', 'z', 'e', 'd' };

    actual_ret = rnc_denormalize(actual_val, sizeof(actual_val), "IX");

    ck_assert_int_eq(0, actual_ret);
    ck_assert_str_eq("VIIII", actual_val);
